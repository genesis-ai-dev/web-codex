{
  "name": "vscode-platform-backend",
  "version": "1.0.0",
  "description": "Backend API for multi-tenant Kubernetes VSCode platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:all": "jest --runInBand",
    "test:watch": "jest --watch --testPathIgnorePatterns=integration",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=integration",
    "test:coverage:all": "jest --coverage --runInBand",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "keywords": [
    "kubernetes",
    "vscode",
    "multi-tenant",
    "platform"
  ],
  "author": "VSCode Platform Team",
  "license": "MIT",
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk": "^2.1498.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jose": "^5.1.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.57.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "jiti": "^2.6.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
